#!name=TikTok Unlock
#!desc=Remove TikTok's regional restrictions.
#!system=iosf

[URL Rewrite]
(?<=_region=)TW(?=&) JP 307
(?<=&mcc_mnc=)466 44 307
^(https?:\/\/dm[\w-]+\.\w+\.com\/.+)(\?)(.+) $1$3 302
(^https?:\/\/*\.\w{4}okv.com\/.+&.+)(\d{2}\.3\.\d)(.+) $118.0$3 302

[MITM]
hostname = %APPEND% *.tiktokv.com, *.byteoversea.com, *.tik-tokapi.com